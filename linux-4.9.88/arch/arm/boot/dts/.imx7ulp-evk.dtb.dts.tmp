/ {
 model = "NXP i.MX7ULP EVK";
 compatible = "fsl,imx7ulp-evk", "fsl,imx7ulp", "Generic DT based system";

 aliases {
  gpio4 = &rpmsg_gpio0;
  gpio5 = &rpmsg_gpio1;
 };

 chosen {
  stdout-path = &lpuart4;
 };

 memory {
  device_type = "memory";
  reg = <0x60000000 0x40000000>;
 };

 backlight {
  compatible = "pwm-backlight";
  pwms = <&pwm0 1 50000>;
  brightness-levels = <0 20 25 30 35 40 100>;
  default-brightness-level = <6>;
  status = "okay";
